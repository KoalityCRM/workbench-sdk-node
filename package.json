{
  "name": "@workbenchcrm/sdk",
  "version": "1.2.0",
  "description": "Official Node.js SDK for the Workbench CRM API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "workbench",
    "crm",
    "api",
    "sdk",
    "client",
    "invoices",
    "quotes",
    "jobs"
  ],
  "author": "Workbench CRM",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KoalityCRM/workbench-sdk-node.git"
  },
  "homepage": "https://docs.tryworkbench.app",
  "bugs": {
    "url": "https://github.com/KoalityCRM/workbench-sdk-node/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}
